<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
<!-- 用户注册的配置 -->
		
	  <servlet>	    
	    <servlet-name>User_Info</servlet-name>
	    <servlet-class>com.jkx.yang.servlet.User_Info</servlet-class>
	  </servlet>
 
	<servlet>
	    <servlet-name>Message_Info</servlet-name>
	    <servlet-class>com.jkx.yang.servlet.Message_Info</servlet-class>
	 </servlet>
  <servlet>
    <servlet-name>message_comment</servlet-name>
    <servlet-class>com.jkx.yang.servlet.message_comment</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Movie_Info</servlet-name>
    <servlet-class>com.jkx.yang.servlet.Movie_Info</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Movie_play_Info</servlet-name>
    <servlet-class>com.jkx.yang.servlet.Movie_play_Info</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Xunlei_Info</servlet-name>
    <servlet-class>com.jkx.yang.servlet.Xunlei_Info</servlet-class>
  </servlet>




   <servlet-mapping>
	    <servlet-name>User_Info</servlet-name>
	    <url-pattern>/User_Info</url-pattern>
	  </servlet-mapping>
	 <servlet-mapping>
	    <servlet-name>Message_Info</servlet-name>
	    <url-pattern>/Message_Info</url-pattern>
	 </servlet-mapping>
  <servlet-mapping>
    <servlet-name>message_comment</servlet-name>
    <url-pattern>/message_comment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Movie_Info</servlet-name>
    <url-pattern>/Movie_Info</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Movie_play_Info</servlet-name>
    <url-pattern>/Movie_play_Info</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Xunlei_Info</servlet-name>
    <url-pattern>/Xunlei_Info</url-pattern>
  </servlet-mapping>
 <!-- 默认首页 -->
 
	  <welcome-file-list>
	    <welcome-file>index.jsp</welcome-file>
	  </welcome-file-list>
</web-app>
